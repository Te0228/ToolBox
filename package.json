{
  "name": "electron-toolbox",
  "version": "1.0.0",
  "description": "A toolbox app with multiple developer tools",
  "main": "dist-electron/main.js",
  "scripts": {
    "dev": "node scripts/copy-wllama.mjs && concurrently \"vite\" \"tsc -p tsconfig.electron.json && wait-on http://localhost:5173 && cross-env NODE_ENV=development electron .\"",
    "build": "node scripts/copy-wllama.mjs && tsc -p tsconfig.electron.json && vite build",
    "preview": "vite preview",
    "electron": "electron .",
    "electron:dev": "cross-env NODE_ENV=development electron ."
  },
  "keywords": [
    "electron",
    "toolbox",
    "dev-tools"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^28.0.0",
    "electron-builder": "^24.9.1",
    "typescript": "^5.3.3",
    "vite": "^5.4.21",
    "wait-on": "^7.2.0"
  },
  "dependencies": {
    "@codemirror/commands": "^6.2.4",
    "@codemirror/lang-json": "^6.0.1",
    "@codemirror/language": "^6.9.1",
    "@codemirror/view": "^6.21.3",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@monaco-editor/react": "^4.7.0",
    "@mui/icons-material": "^7.3.5",
    "@mui/material": "^7.3.5",
    "@uiw/react-codemirror": "^4.21.21",
    "@wllama/wllama": "^2.3.6",
    "codemirror": "^6.0.1",
    "monaco-editor": "^0.55.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
